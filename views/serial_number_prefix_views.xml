<odoo>
    <record id="view_serial_number_prefix_form" model="ir.ui.view">
        <field name="name">serial.number.prefix.form</field>
        <field name="model">serial.number.prefix</field>
        <field name="arch" type="xml">
            <form string="Serial Number Prefix">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="prefix"/>
                        <field name="serial_numbers" widget="one2many_list"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>